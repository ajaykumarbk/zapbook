pipeline {
    agent any
    stages {
        stage('Clone') {
            steps {
                script {
                    echo "Cloning application"
                }
            }
        }

        stage('build') {
            steps {
                script {
                    echo "Building application"
                }
            }
        }

        stage('test') {
            steps {
                script {
                    echo "Testing application"
                }
            }
        }

        stage('deploy') {
            steps {
                script {
                    echo "Deploying application"
                }
            }
        }
    }
}
